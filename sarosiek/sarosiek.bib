	editor = {van de Vijver, Gertrudis and Salthe, Stanley N. and Delpos, Manuela},
	year = {1998},
	doi = {10.1007/978-94-017-1510-2_21},
	keywords = {Digital Code, Linguistic Mode, Logical Depth, Maximum Entropy Production, Semiotic System},
	pages = {281--293},
	file = {Springer Full Text PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/CQCRESRQ/Hoffmeyer - 1998 - The Unfolding Semiosphere.pdf:application/pdf},
